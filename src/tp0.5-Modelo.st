Object subclass: #Club	instanceVariableNames: 'nombre tarifaSocio tarifaSocioVip socios sociosVip recaudacionTotal pagos'	classVariableNames: ''	package: 'tp0.5-Modelo'!!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/18/2020 19:51'!establecerNombre: unNombre 	nombre := unNombre.! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 19:42'!agregarSocioVIP: unSocio	"Cambia a Vip el socio y lo agrega a la OrderedCollection de socios		tambien agrega el nombre a la lista de pagos"	unSocio cambiarAVipConCuota:tarifaSocioVip . 	socios add: unSocio.	pagos at: (unSocio getNombre) put: 0.! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 19:42'!agregarSocio: unSocio 	socios add: (unSocio establecerCouta: tarifaSocio).	pagos at: (unSocio getNombre) put: 0.! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 18:11'!nombresSocios	"Devuelve todos los socios , tanto vips como normales"	| nombres |	nombres := socios collect: [ :socio | socio getNombre].		^ nombres! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 19:44'!cantidadPagadaPor: unSocio 	^ pagos at: unSocio .! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 19:35'!initialize	recaudacionTotal := 0.	socios := OrderedCollection new.	pagos := Dictionary new.	! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/21/2020 17:49'!recaudacionTotal	^ recaudacionTotal .! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/18/2020 19:52'!establecerTarifaSocioVIP: unaTarifaVip 	tarifaSocioVip := unaTarifaVip .! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/18/2020 19:51'!establecerTarifaSocio: unaTarifa 	tarifaSocio := unaTarifa.! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 19:50'!agregarPago: unNombreDeSocio conCouta: unacuotaClub	| montoActual |		montoActual := pagos at: unNombreDeSocio.	pagos at: unNombreDeSocio  put: (montoActual + unacuotaClub ).! !!Club methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/21/2020 17:51'!cantidadDeSocios	^(self nombresSocios size) ! !!Club methodsFor: 'accessing' stamp: 'AlejoVillores 10/22/2020 19:48'!recibirPagoDe: unNombreDeSocio	| elSociox |		elSociox := socios detect: [ :socio | socio getNombre = unNombreDeSocio ].	recaudacionTotal :=  recaudacionTotal + (elSociox cuotaClub).		self agregarPago:unNombreDeSocio conCouta:(elSociox cuotaClub).! !Object subclass: #Socio	instanceVariableNames: 'nombre membresia coutaClub'	classVariableNames: ''	package: 'tp0.5-Modelo'!!Socio methodsFor: 'accessing' stamp: 'AlejoVillores 10/22/2020 18:32'!membresia	^ membresia socio.! !!Socio methodsFor: 'initialization' stamp: 'AlejoVillores 10/22/2020 18:35'!initialize	| socioNormal |	socioNormal := SocioNoVip  new.	membresia := socioNormal .! !!Socio methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/18/2020 19:53'!establecerNombre: unNombre  	nombre := unNombre ! !!Socio methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/18/2020 20:00'!getNombre	^ nombre! !!Socio methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 18:42'!cambiarAVipConCuota: unaCuota 	| socioVip | 	socioVip  := SocioVip new.	self establecerCouta: unaCuota. 	membresia := socioVip .! !!Socio methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 18:39'!cuotaClub	^ coutaClub! !!Socio methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 18:42'!establecerCouta: unaCouta	coutaClub := unaCouta! !Object subclass: #SocioNoVip	instanceVariableNames: ''	classVariableNames: ''	package: 'tp0.5-Modelo'!!SocioNoVip methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 18:34'!socio	^ false.! !Object subclass: #SocioVip	instanceVariableNames: ''	classVariableNames: ''	package: 'tp0.5-Modelo'!!SocioVip methodsFor: 'as yet unclassified' stamp: 'AlejoVillores 10/22/2020 18:32'!socio	^ true.! !